program 1

motorcycles = ['honda', 'yamaha', 'suzuki']
print(motorcycles)


output
['honda', 'yamaha', 'suzuki']


program 2
motorcycles = ['honda', 'yamaha', 'suzuki']
print(motorcycles)
motorcycles.append('ducati')
print(motorcycles)


output
['honda', 'yamaha', 'suzuki']
['honda', 'yamaha', 'suzuki', 'ducati']

program 3

motorcycles = []
motorcycles.append('honda')
motorcycles.append('yamaha')
motorcycles.append('suzuki')
print(motorcycles)

output
['honda', 'yamaha', 'suzuki']


