program 1

motorcycles = ['honda', 'yamaha', 'suzuki']
print(motorcycles)
del motorcycles[0]
print(motorcycles)

output
['honda', 'yamaha', 'suzuki']
['yamaha', 'suzuki']


program 2

motorcycles = ['honda', 'yamaha', 'suzuki']
print(motorcycles)
del motorcycles[1]
print(motorcycles)

output
['honda', 'yamaha', 'suzuki']
['honda', 'suzuki']
